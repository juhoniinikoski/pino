{
  "compilerOptions": {
    "target": "ES6",
    "outDir": "./build/",
    "module": "commonjs",
    "strict": true,
    "strictNullChecks": false,
    // "noUnusedLocals": true,
    "allowJs": true,
    // "noUnusedParameters": true,
    // "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "types": [ "node" ]
  },
  "exclude":[
    "./node_modules"
  ]
}